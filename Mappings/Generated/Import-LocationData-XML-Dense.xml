<?xml version="1.0"?>
<AimmsXMLMapping>
	<ElementObjectMapping name="LocationData">
		<ElementObjectMapping name="Row" dense-children="1">
			<AttributeMapping name="Location" binds-to="i_loc" />
			<ElementValueMapping name="Latitude" maps-to="p_latitude(i_loc)" />
			<ElementValueMapping name="Longitude" maps-to="p_longitude(i_loc)" />
			<ElementValueMapping name="Idle Cost" maps-to="p_idleCostLocation(i_loc)" />
		</ElementObjectMapping>
	</ElementObjectMapping>
</AimmsXMLMapping>
