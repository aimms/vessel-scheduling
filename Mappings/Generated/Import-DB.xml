<?xml version="1.0"?>
<AimmsDatabaseMapping>
	<TableMapping name="CargoData">
		<RowMapping>
			<ColumnMapping name="Cargo" binds-to="i_cargo" />
			<ColumnMapping name="Spotcost on Vessel" maps-to="p_spotCostVessel(i_cargo)" />
			<ColumnMapping name="Minimum Loading Time" maps-to="ep_minTimeWindow(i_cargo)" />
			<ColumnMapping name="Maximum Loading Time" maps-to="ep_maxTimeWindow(i_cargo)" />
			<ColumnMapping name="Loading Port" maps-to="ep_loadingPortsCargo(i_cargo)" />
			<ColumnMapping name="Delevering Port" maps-to="ep_deliveringPortsCargo(i_cargo)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="CargoCosts">
		<RowMapping>
			<ColumnMapping name="Cargo" binds-to="i_cargo" />
			<ColumnMapping name="Cost Type" binds-to="i_ct" />
			<ColumnMapping name="Type Costs" maps-to="p_cargoCost(i_cargo,i_ct)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="VesselData">
		<RowMapping>
			<ColumnMapping name="Vessel" binds-to="i_vessel" />
			<ColumnMapping name="Port of Origin" maps-to="ep_originPortOfVessel(i_vessel)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="VesselCosts">
		<RowMapping>
			<ColumnMapping name="Vessel" binds-to="i_vessel" />
			<ColumnMapping name="Cost Type" binds-to="i_ct" />
			<ColumnMapping name="DailyCost" maps-to="p_vesselCost(i_vessel,i_ct)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="Type Cost Group">
		<RowMapping>
			<ColumnMapping name="Cost Type" binds-to="i_ct" />
			<ColumnMapping name="Cargoes" maps-to="bp_activeCostsForCargoes(i_ct)" />
			<ColumnMapping name="Vessel" maps-to="bp_activeCostsForVessels(i_ct)" />
			<ColumnMapping name="Location" maps-to="bp_activeCostsForLocations(i_ct)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="LocationData">
		<RowMapping>
			<ColumnMapping name="Location" binds-to="i_loc" />
			<ColumnMapping name="Latitude" maps-to="p_latitude(i_loc)" />
			<ColumnMapping name="Longitude" maps-to="p_longitude(i_loc)" />
			<ColumnMapping name="Idle Cost" maps-to="p_idleCostLocation(i_loc)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="LocationCosts">
		<RowMapping>
			<ColumnMapping name="Location" binds-to="i_loc" />
			<ColumnMapping name="Cost Type" binds-to="i_ct" />
			<ColumnMapping name="Costs" maps-to="p_locationCost(i_loc,i_ct)" />
		</RowMapping>
	</TableMapping>
	<TableMapping name="DistanceBetweenLocations">
		<RowMapping>
			<ColumnMapping name="Location (From)" binds-to="i_loc_from" />
			<ColumnMapping name="Location (To)" binds-to="i_loc_to" />
			<ColumnMapping name="DeleveringLocation" maps-to="p_distanceBetweenLocations(i_loc_from,i_loc_to)" />
		</RowMapping>
	</TableMapping>
</AimmsDatabaseMapping>
